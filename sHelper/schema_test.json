				"message-id": {
					"type": "string",
					"title": "The message-id schema"
				},
				"aws-user-name": {
					"type": "string",
					"title": "The aws-user-name schema"
				},
				"organizations-id": {
					"type": "integer",
					"title": "The organizations-id schema"
				},
				"role-list": {
					"type": "array",
					"title": "The role-list schema",
					"enum": ["CLIENT_ADMIN", "EXECUTIVE"]
				}
			}
		}
	}
}

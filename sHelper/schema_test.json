{
	"$schema": "http://json-schema.org/draft-07/schema",
  	"type": "object",
    "required": [
        "field1",
        "field2"
    ],
    "properties": {
		"request-header": {
			"$ref": "#/definitions/request-header"
		},
        "field1": {
            "$id": "#/properties/field1",
            "type": "string"
        },
        "field2": {
            "$id": "#/properties/field2",
            "type": "string"
        },
        "field3": {
            "$id": "#/properties/field3",
            "type": "string",
            "default": "VALUE1",
            "enum": ["VALUE1", "VALUE2"]
        }
